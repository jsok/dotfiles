[alias]
  st = status
  di = diff
  out = diff --cached --submodule
  in = fetch --dry-run
  ci = commit
  fixup = commit --fixup
  br = branch
  bra = branch -a
  co = checkout
  rm = remote -v
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  unstage = reset HEAD
  squash = rebase -i --autosquash
  repo = !hub browse
  browse = "!hub browse -- blob/$(git rev-parse HEAD)/\"$1\" #"
  pull-request = !hub pull-request
  prs = !hub browse -- pulls
  issues = !hub browse -- issues
  pages = !open $(gh-pages-url)
  jenkins = !open $(jenkins-url)
  root = rev-parse --show-toplevel
[branch]
  autosetupmerge = true
[color]
  diff = auto
  status = auto
  branch = auto
  ui = auto
[core]
  excludesfile = ~/.gitignore
  filemode = true
[diff]
  compactionHeuristic = true
  submodule = log
[help]
  autocorrect = 1
[include]
  path = .gitconfig.local
[push]
  default = tracking
[remote]
  prune = true
[fetch]
  prune = true
[credential]
  helper = osxkeychain
