# Set Apple Terminal.app resume directory
if [[ $TERM_PROGRAM == "Apple_Terminal" ]] && [[ -z "$INSIDE_EMACS" ]] {
  function chpwd {
    local SEARCH=' '
    local REPLACE='%20'
    local PWD_URL="file://$HOSTNAME${PWD//$SEARCH/$REPLACE}"
    printf '\e]7;%s\a' $PWD_URL
    }

    chpwd
}

alias tmux="tmux -2"
alias ll="ls -al"
alias cdd="cd .."
alias cls="clear"
alias vi="vim"
alias cdd="cd ../"
alias usage="du -h -d 1"
